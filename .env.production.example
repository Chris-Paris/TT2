# Stripe API keys (server-side for Netlify Functions)
# Replace with your production secret key (starts with sk_live_)
STRIPE_SECRET_KEY=sk_live_your_production_key

# Replace with your production webhook secret (if applicable)
STRIPE_WEBHOOK_SECRET=whsec_your_production_webhook_secret

# Site URL for production
VITE_SITE_URL=https://www.travellingtrip.com

# Frontend environment variables (client-side)
# These are prefixed with VITE_ to make them accessible in the frontend
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_PEXELS_API_KEY=your_pexels_api_key
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server-side environment variables for Netlify Functions
# These will NOT be exposed to the client
GEMINI_API_KEY=your_gemini_api_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
